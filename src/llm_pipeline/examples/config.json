{
  "default_model": "llama3.1-8b",
  "default_executor": "cerebras",
  "template_namespace": "templates",
  "timeout_seconds": 60,
  "max_output_tokens": 256,
  "json_retry_attempts": 2,
  "output_schema": {
    "type": "object",
    "properties": {
      "answer": { "type": "string" },
      "confidence": { "type": "number", "minimum": 0, "maximum": 1 }
    },
    "required": ["answer"],
    "additionalProperties": false
  }
}
